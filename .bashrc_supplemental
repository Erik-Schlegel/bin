# 1. Populate the variables.
# 2. Source this from ~/.bashrc.

PS1="\w: "

export NGROK_UN=epschlegel
export NGROK_PW=!_nanite.Paroxysm
export NGROK_OAUTH_EMAIL=epschlegel@gmail.com
export NGROK_SUBDOMAIN=esch

export PATH=$HOME/bin:$PATH

# Github SSH.
env=~/.ssh/agent.env
agent_load_env() { test -f "$env" && . "$env" >|/dev/null; }
agent_start() {
    (
        umask 077
        ssh-agent >|"$env"
    )
    . "$env" >|/dev/null
}
agent_load_env
agent_run_state=$(
    ssh-add -l >|/dev/null 2>&1
    echo $?
)
if [ ! "$SSH_AUTH_SOCK" ] || [ $agent_run_state = 2 ]; then
    agent_start
    ssh-add
elif [ "$SSH_AUTH_SOCK" ] && [ $agent_run_state = 1 ]; then
    ssh-add
fi
unset env
